C.js can generate 3D scenes because it is compatible with 3D libraries such\r\nas Three.js or Babylon.js. This means that you can create 3D BIM tools right\r\naway.","position":{"start":{"line":82,"column":3,"offset":3955},"end":{"line":84,"column":8,"offset":4121}}}],"position":{"start":{"line":82,"column":3,"offset":3955},"end":{"line":84,"column":8,"offset":4121}}}],"position":{"start":{"line":81,"column":1,"offset":3915},"end":{"line":85,"column":11,"offset":4133}}},{"type":"mdxFlowExpression","value":"\" \"","position":{"start":{"line":87,"column":1,"offset":4137},"end":{"line":87,"column":6,"offset":4142}},"data":{"estree":{"type":"Program","start":4138,"end":4141,"body":[{"type":"ExpressionStatement","expression":{"type":"Literal","start":4138,"end":4141,"value":" ","raw":"\" \"","loc":{"start":{"line":87,"column":1,"offset":4138},"end":{"line":87,"column":4,"offset":4141}},"range":[4138,4141]},"start":4138,"end":4141,"loc":{"start":{"line":87,"column":1,"offset":4138},"end":{"line":87,"column":4,"offset":4141}},"range":[4138,4141]}],"sourceType":"module","comments":[],"loc":{"start":{"line":87,"column":1,"offset":4138},"end":{"line":87,"column":4,"offset":4141}},"range":[4138,4141]}}},{"type":"mdxJsxFlowElement","name":"IfcCard","attributes":[{"type":"mdxJsxAttribute","name":"icon","value":"üîó"},{"type":"mdxJsxAttribute","name":"title","value":"Data"}],"children":[{"type":"paragraph","children":[{"type":"text","value":"High-level access to all the properties associated with that geometry. This\r\nmeans easy access to data on the building components, their materials, thermal\r\ncharacteristics, structural strength, etc.","position":{"start":{"line":90,"column":3,"offset":4182},"end":{"line":92,"column":45,"offset":4385}}}],"position":{"start":{"line":90,"column":3,"offset":4182},"end":{"line":92,"column":45,"offset":4385}}}],"position":{"start":{"line":89,"column":1,"offset":4146},"end":{"line":93,"column":11,"offset":4397}}},{"type":"paragraph","children":