page.","position":{"start":{"line":130,"column":1,"offset":5665},"end":{"line":130,"column":388,"offset":6052}}}],"position":{"start":{"line":130,"column":1,"offset":5665},"end":{"line":130,"column":388,"offset":6052}}},{"type":"heading","depth":3,"children":[{"type":"text","value":"Multiplatform","position":{"start":{"line":132,"column":5,"offset":6060},"end":{"line":132,"column":18,"offset":6073}}}],"position":{"start":{"line":132,"column":1,"offset":6056},"end":{"line":132,"column":18,"offset":6073}}},{"type":"paragraph","children":[{"type":"text","value":"You can use IFC.js to create open BIM applications for any platform:","position":{"start":{"line":134,"column":1,"offset":6077},"end":{"line":134,"column":69,"offset":6145}}}],"position":{"start":{"line":134,"column":1,"offset":6077},"end":{"line":134,"column":69,"offset":6145}}},{"type":"mdxFlowExpression","value":"\" \"","position":{"start":{"line":136,"column":1,"offset":6149},"end":{"line":136,"column":6,"offset":6154}},"data":{"estree":{"type":"Program","start":6150,"end":6153,"body":[{"type":"ExpressionStatement","expression":{"type":"Literal","start":6150,"end":6153,"value":" ","raw":"\" \"","loc":{"start":{"line":136,"column":1,"offset":6150},"end":{"line":136,"column":4,"offset":6153}},"range":[6150,6153]},"start":6150,"end":6153,"loc":{"start":{"line":136,"column":1,"offset":6150},"end":{"line":136,"column":4,"offset":6153}},"range":[6150,6153]}],"sourceType":"module","comments":[],"loc":{"start":{"line":136,"column":1,"offset":6150},"end":{"line":136,"column":4,"offset":6153}},"range":[6150,6153]}}},{"type":"mdxJsxFlowElement","name":"IfcTab","attributes":[{"type":"mdxJsxAttribute","name":"items","value":{"type":"mdxJsxAttributeValueExpression","value":"[\r\n {\r\n   icon: \"üåè\",\r\n   content: (\r\n     <p>\r\n       <b>Frontend web applications</b> that read and write IFC files and\r\n       display 3D without relying on server communication can be created\r\n       using <b>vanilla JavaScript</b> or other tools such as{\" \"}\r\n       <b>React, Vue, Angular, Svelte, etc</b>. That is,{\" \"}\r\n       <i>with IFC.js we can turn any web browser into an open BIM app.</i>\r\n     </p>\r\n   ),\r\n },\r\n {\r\n   icon: \"üè¢\",\r\n   content: (\r\n     <p>\r\n       There are cases where the IFC file cannot be processed by the client.\r\n       For example, a mobile device may not have the power to display several\r\n       medium or large IFCs. In this case, it is possible to{\" \"}\r\n       <b>use IFC.js on the server</b> via{\" \"}\r\n       <a href=\"https://nodejs.org/en/\">Node.js</a> and send the Three.js\r\n       scene already prepared to the client.\r\n     </p>\r\n   ),\r\n },\r\n {\r\n   icon: \"üíª\",\r\n   content: (\r\n     <p>\r\n       Sometimes we don't want to create web applications, but{\" \"}\r\n       <b>native desktop applications that run on Windows, iOS and Linux</b>.\r\n       This is possible thanks to technologies like{\" \"}\r\n       <a href=\"https://www.electronjs.org/\">Electron</a>. These native\r\n       applications will be programmed in the same way as web applications;\r\n       HTML, CSS, JavaScript, React, Vue, etc.\r\n     </p>\r\n   ),\r\n },\r\n {\r\n   icon: \"üì±\",\r\n   content: (\r\n     <p>\r\n       IFC.js also supports{\" \"}\r\n       <a href=\"https://reactnative.dev/\">React Native</a>, which maps WebGL\r\n       code to native OpenGL. This means that you can create IFC-compatible{\" \"}\r\n       <b>Open BIM apps that run on Android and iOS</b> easily.\r\n     </p>\r\n   ),\r\n },\r\n]","data":{"estree":{"type":"Program","start":6176,"end":8015,"body":[{"type":"ExpressionStatement","expression":{"type":"ArrayExpression","start":6176,"end":8015,"loc":{"start":{"line":139,"column":9,"offset":6176},"end":{"line":189,"column":3,"offset":8015}},"elements":[{"type":"ObjectExpression","start":6183,"end":6637,"loc":{"start":{"line":140,"column":4,"offset":6183},"end":{"line":151,"column":5,"offset":6637}},"p